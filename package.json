    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@colyseus/core": "0.16.24",
    "@colyseus/redis-driver": "^0.16.1",
    "@colyseus/redis-presence": "^0.16.4",
    "@colyseus/schema": "^3.0.0",
    "@colyseus/ws-transport": "0.16.5",
    "@supabase/supabase-js": "2.47.10",
    "colyseus": "0.16.5",
    "compression": "1.7.5",
    "cors": "2.8.5",
    "express": "4.21.2",
    "three": "0.170.0",
    "ws": "8.18.0"
  },
  "devDependencies": {
    "@types/compression": "1.7.5",
    "@types/cors": "2.8.17",
    "@types/express": "5.0.0",
    "@types/node": "22.10.5",
    "@types/three": "0.170.0",
    "concurrently": "9.1.2",
    "tsx": "4.19.2",
    "typescript": "5.7.2",
    "vite": "6.0.7",
    "vite-plugin-compression": "0.5.1"
  }
